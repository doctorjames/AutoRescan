<extensions>
  <details>
    <title lang="EN">AutoRescan Plugin</title>
  </details>
  <plugins>
    <plugin name="AutoRescan" version="@@VERSION@@" minTarget="7.7" maxTarget="*" target="windows|unix">
      <title lang="EN">AutoRescan</title>
      <desc lang="EN">AutoRescan detects changes to your music folder and automatically triggers a rescan of that directory if any files have been added, removed or changed.</desc>
      <url>https://github.com/doctorjames/AutoRescan/raw/master/releases/@@DISTFILE@@</url>
      <sha>@@SHASUM@@</sha>
      <creator>James Marsh; Stuart Hickinbottom</creator>
    </plugin>
  </plugins>
</extensions>
